---
layout: null
---
<?xml version="1.0" encoding="UTF-8"?>
<urlset xmlns="http://www.sitemaps.org/schemas/sitemap/0.9">
  {% for page in site.pages %}
    {% if page.sitemap.include %}
    <url>
      <loc>{{ site.url }}{{ page.url | remove: "index.html" }}</loc>
        <lastmod>{{ page.sitemap.lastmod | default: page.date | default: site.time | date: "%Y-%m-%d" }}</lastmod>
        <changefreq>{{ page.sitemap.changefreq | default: "monthly" }}</changefreq>
        <priority>{{ page.sitemap.priority | default: 0.5 }}</priority>
    </url>
    {% endif %}
  {% endfor %}
</urlset>
